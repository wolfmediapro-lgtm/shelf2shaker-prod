# === Shelf2Shaker Environment Template ===
# Copy this file to .env (which is ignored by Git)
# Then fill in your real Firebase config values.

# Firebase configuration
VITE_API_KEY=YOUR_FIREBASE_API_KEY
VITE_AUTH_DOMAIN=shelf2shaker-prod.firebaseapp.com
VITE_PROJECT_ID=shelf2shaker-prod
VITE_STORAGE_BUCKET=shelf2shaker-prod.appspot.com
VITE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID
VITE_APP_ID=YOUR_APP_ID
VITE_MEASUREMENT_ID=YOUR_MEASUREMENT_ID

# Optional analytics or ad configuration
VITE_GA_MEASUREMENT_ID=YOUR_GOOGLE_ANALYTICS_ID
VITE_ADSENSE_CLIENT_ID=YOUR_ADSENSE_CLIENT_ID

# Other environment variables
VITE_ENV=development
VITE_DEBUG=true

